- freckle:
   owner: mysql
   group: mysql
   staging_method: stow
   stow_root: /var/lib

- mysql:
   # mysql_root_password: secret
   # mysql_root_password_update: no
   mysql_databases:
     - name: wp_database
   mysql_users:
     - name: wp_user
       host: localhost
       password: wp_user_password
       priv: "wp_database.*:ALL"



