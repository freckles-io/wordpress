- freckle:
   owner: www-data
   group: www-data
   staging_method: stow
   stow_root: /var/www/wordpress

- wordpress:
   # letsencrypt_email: <your_email>  # if specified, a https certificate will be downloaded, installed and configured
   wordpress_domain: 'localhost'
   wordpress_port: 80
   wordpress_db_name: wp_database
   wordpress_db_host: localhost
   wordpress_db_user: wp_user
   wordpress_db_user_password: wp_user_password
   wordpress_db_table_prefix: wp_
   wordpress_language: en_US
   wordpress_debug: false
   wordpress_letsencrypt_staging: false
