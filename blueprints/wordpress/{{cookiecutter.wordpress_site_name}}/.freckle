- vagrant-dev:
    __auto_run__: false
    vagrant_dev_plugins:
       - vagrant-triggers

- docker-dev:
    __auto_run__: false
    docker_ports:
       - "8280:8280"
    docker_volumes:
       "wordpress":
         target: /freckle/wordpress
       "mysql_backup":
         target: /freckle/mysql_backup
       "mysql_current":
         target: /var/lib/mysql
